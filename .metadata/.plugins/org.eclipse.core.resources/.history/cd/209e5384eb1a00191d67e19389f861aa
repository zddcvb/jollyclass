package com.jollyclass.airplayer.constant
{
	/**
	 * 播放器错误信息汇总，通过发送错误码，判定错误的类型
	 */
	public class ErrorMsgNumber
	{
		/**
		 * jx01:系统app发送数据不符合规范
		 */
		private static const PARSE_DATA_ERROR:String="jx01";
		/**
		 * jx02:系统app未发送数据给播放器！
		 */
		private static const INOVKE_DATA_LENGTH_ERROR:String="jx02";
		/**
		 * jx03:所需播放的课件不存在！
		 */
		private static const FILE_NOT_EXITS:String="jx03";
		/**
		 * jx04:课件读取失败！
		 */
		private static const FILE_READ_ERROR:String="jx04";
		/**
		 * jx05:swf的字节长度太短，无法加载！
		 */
		private static const SWF_BYTE_LENGTH_ERROR:String="jx05";
		/**
		 * jx06:加载swf文件失败！
		 */
		private static const LOAD_SWF_ERROR:String="jx06";
		public function ErrorMsgNumber()
		{
		}
	}
}